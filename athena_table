SELECT eventtime, eventname,
       json_extract_scalar(requestparameters,'$.bucketName') AS bucket,
       json_extract_scalar(requestparameters,'$.key')        AS object_key
FROM ct_logs.cloudtrail_events
WHERE eventsource='s3.amazonaws.com' AND eventcategory='Data'
  AND region='us-east-1' AND year='2025' AND month='08' AND day='01'   -- adjust to a day with events
LIMIT 50;
