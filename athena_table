SELECT
  format(
    's3://aws-cloudtrail-logs-<account-id>/AWSLogs/<account-id>/CloudTrail/%s/%s/%s/%s/',
    region, year, month, day
  ) AS path_guess,
  COUNT(*) AS events_in_prefix
FROM ct_logs.cloudtrail_events
WHERE region='us-east-1'
  AND year='2025'
  AND month='08'
  AND day='16'
GROUP BY 1;
