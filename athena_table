SELECT
  json_extract_scalar(CAST(e.useridentity AS JSON), '$.userName')    AS userName,
  json_extract_scalar(CAST(e.useridentity AS JSON), '$.username')    AS username,
  json_extract_scalar(CAST(e.useridentity AS JSON), '$.sessionContext.sessionIssuer.userName') AS issuerUserName,
  json_extract_scalar(CAST(e.useridentity AS JSON), '$.principalId') AS principalId,
  json_extract_scalar(CAST(e.useridentity AS JSON), '$.arn')         AS arn
FROM ct_logs.cloudtrail_events e
WHERE e.eventsource='s3.amazonaws.com'
  AND e.year='2025' AND e.month='08' AND e.day='16'
LIMIT 5;
