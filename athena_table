SELECT event_date, awsRegion, eventSource, eventName,
       COALESCE(ebs_snapshot_id, rds_snapshot_id, rds_cluster_snapshot_id) AS snapshot_id,
       COALESCE(ebs_volume_id, rds_instance_id, rds_cluster_id)            AS source_resource,
       actor_arn, event_ts
FROM ( /* same CTEs as above */ ct )
JOIN dates d ON ct.event_date = d.d
WHERE eventName IN ('CreateSnapshot','CreateSnapshots','CreateDBSnapshot','CreateDBClusterSnapshot')
ORDER BY event_ts;
